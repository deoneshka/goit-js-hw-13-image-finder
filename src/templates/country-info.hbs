{{#each this}}
<div class="wrapper-name-flag">
    <img class="country-flag" src="{{flag}}" alt="{{name}} flag">
    <h2 class="country-name">{{name}}</h2>
</div>
<div class="country-info-wrapper">
    <p class="country-capital">Capital: {{capital}}</p>
    <p class="country-population">Population: {{population}}</p>
    <p class="country-languages">Languages:
    <ul class="country-languages-list">
        {{#each languages}}
        <li class="country-languages-item">{{name}}</li>
        {{/each}}
    </ul>
    </p>
</div>
{{/each}}